<div class="page-header">
  <%= link_to new_prenotation_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Prenotation
  <% end %>
  <h1>Listing prenotations</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Name</th>
            <th>Start time</th>
            <th>End time</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @prenotations.each do |prenotation| %>
        <%= content_tag :tr, id: dom_id(prenotation), class: dom_class(prenotation) do %>
                      <td><%= prenotation.name %></td>
                      <td><%= prenotation.start_time %></td>
                      <td><%= prenotation.end_time %></td>
                    <td><%= link_to 'Show', prenotation %></td>
          <td><%= link_to 'Edit', edit_prenotation_path(prenotation) %></td>
          <td><%= link_to 'Destroy', prenotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
