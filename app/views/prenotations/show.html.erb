<div class="page-header">
  <%= link_to prenotations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_prenotation_path(@prenotation), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show prenotation</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @prenotation.name %></dd>

  <dt>Start time:</dt>
  <dd><%= @prenotation.start_time %></dd>

  <dt>End time:</dt>
  <dd><%= @prenotation.end_time %></dd>

</dl>
